import javax.swing.*;

public class ch2p35
{
    public static void main(String args[])
    {
	String[] sNums = new String[5]; 
	int[] nums = new int[5];
	
	for(int x = 0; x < 5; x++)
	    {
		sNums[x] = JOptionPane.showInputDialog("Enter number " + x);
		nums[x] = Integer.parseInt(sNums[x]);
	    }
	for(int x = 0; x < 5; x++)
	    {
		System.out.println(nums[x]);
	    }
    }
}
